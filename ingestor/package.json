{
  "name": "realestate-ingestor",
  "version": "1.0.0",
  "description": "Real estate listing ingestor for TRREB/CREA data",
  "main": "src/bin/start.ts",
  "scripts": {
    "dev": "ts-node src/bin/start.ts",
    "dev:full": "npm run start:all",
    "start:all": "node scripts/start-all.js",
    "start:db": "docker compose up -d db",
    "stop:db": "docker compose down",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "build": "tsc",
    "start": "node dist/bin/start.js"
  },
  "keywords": [
    "real-estate",
    "mls",
    "trreb",
    "crea",
    "ingestor"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
